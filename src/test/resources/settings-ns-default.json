{
  "namespace": {
    "name": "default",
    "status": "ENABLED"
  },
  "filters": [
    {
      "name": "userNameContains",
      "parameter": "userName",
      "operator": "contains",
      "description": "Фильтр по имени пользователя (можно внести список имен пользователей, на которые будет распространена фича, спецсимволы применять нельзя)"
    },
    {
      "name": "osEq",
      "parameter": "os",
      "operator": "eq",
      "description": "Фильтр по типу операционной системы"
    },
    {
      "name": "osVersionMore",
      "parameter": "osVersion",
      "operator": "more"
    }
  ],
  "features": [
    {
      "name": "enablePayments",
      "status": "ENABLED",
      "filters": [
        {
          "name": "osEq",
          "value": "ios"
        },
        {
          "name": "osVersionMore",
          "value": "13"
        }
      ],
      "description": "Включает payments для новых версий"
    },
    {
      "name": "enableAdvancedScroll",
      "status": "ENABLED",
      "filters": [
        {
          "name": "osEq",
          "value": "ios"
        },
        {
          "name": "userNameContains",
          "value": "patrik crabs"
        }
      ]
    }
  ]
}